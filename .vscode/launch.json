{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "msedge",
      "request": "launch",
      "name": "Nuxt: Client (msedge)",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Nuxt: Server",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "dev"
      ],
      "console": "integratedTerminal"
    }
  ],
  "compounds": [
    {
      "name": "Nuxt: Full Stack",
      "configurations": [
        "Nuxt: Server",
        "Nuxt: Client (msedge)"
      ]
    }
  ]
}